{
  "name": "taro3-demo",
  "version": "1.5.0",
  "private": true,
  "description": "allen-nutui-demo",
  "templateInfo": {
    "name": "vue3-NutUI",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build": "npm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev": "npm run dev:weapp",
    "dev:weapp": "npm run build:weapp -- --watch dev",
    "dev:swan": "npm run build:swan -- --watch dev",
    "dev:alipay": "npm run build:alipay -- --watch dev",
    "dev:tt": "npm run build:tt -- --watch dev",
    "dev:h5": "npm run build:h5 -- --watch dev",
    "dev:rn": "npm run build:rn -- --watch dev",
    "dev:qq": "npm run build:qq -- --watch dev",
    "dev:quickapp": "npm run build:quickapp -- --watch dev",
    "beta": "npm run beta:weapp",
    "beta:weapp": "npm run build:weapp -- --watch beta",
    "beta:swan": "npm run build:swan -- --watch beta",
    "beta:alipay": "npm run build:alipay -- --watch beta",
    "beta:tt": "npm run build:tt -- --watch beta",
    "beta:h5": "npm run build:h5 -- --watch beta",
    "beta:rn": "npm run build:rn -- --watch beta",
    "beta:qq": "npm run build:qq -- --watch beta",
    "beta:quickapp": "npm run build:quickapp -- --watch beta",
    "prod": "npm run prod:weapp",
    "prod:weapp": "npm run build:weapp -- --watch prod",
    "prod:swan": "npm run build:swan -- --watch prod",
    "prod:alipay": "npm run build:alipay -- --watch prod",
    "prod:tt": "npm run build:tt -- --watch prod",
    "prod:h5": "npm run build:h5 -- --watch prod",
    "prod:rn": "npm run build:rn -- --watch prod",
    "prod:qq": "npm run build:qq -- --watch prod",
    "prod:quickapp": "npm run build:quickapp -- --watch prod",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint --ext .vue --ext .js --ext .ts src/ --fix",
    "style": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "log": "standard-version",
    "cz": "git add . && git-cz"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/nutui-taro": "^3.1.10",
    "@tarojs/components": "3.3.15",
    "@tarojs/plugin-html": "3.3.15",
    "@tarojs/runtime": "3.3.15",
    "@tarojs/taro": "3.3.15",
    "pinia": "^2.0.5",
    "taro-plugin-pinia": "^1.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "@tarojs/cli": "3.3.15",
    "@tarojs/mini-runner": "3.3.15",
    "@tarojs/webpack-runner": "3.3.15",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.3.15",
    "commitlint": "^15.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-taro": "3.3.15",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "standard-version": "^9.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.1.0",
    "vue-loader": "^16.0.0-beta.8",
    "vue-tsc": "^0.29.3"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
